{
  "expo": {
    "name": "MimirAppScreens",
    "slug": "MimirAppScreens",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "wc",
    "icon": "assets/image/app-icon.png",
    "splash": {
      "image": "assets/image/app-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#222222"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mimir.quiz"
    },
    "android": {
      "package": "com.mimir.quiz"
    }
  }
}

