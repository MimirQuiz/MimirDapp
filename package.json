    "ios": "node_modules/.bin/ts-node scripts/ios",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/MimirDapp/main.jsbundle' --dev=false --platform='ios' --assets-dest='./iOS'",
